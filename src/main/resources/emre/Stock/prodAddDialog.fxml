            <Label prefHeight="17.0" prefWidth="188.0" text="Eklenecek parça sayısını giriniz" />
            <TextField fx:id="addedNumber" prefHeight="25.0" prefWidth="188.0" promptText="Parça Adet">
               <VBox.margin>
                  <Insets left="50.0" right="50.0" />
               </VBox.margin>
            </TextField>
